    "next": "^10.0.1",
    "next-auth": "^3.1.0",
    "next-images": "^1.6.0",
    "pg": "^8.5.1",
    "postcss": "^8.1.9",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-draft-wysiwyg": "^1.14.5",
